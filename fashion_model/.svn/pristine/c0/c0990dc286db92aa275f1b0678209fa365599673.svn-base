main > .container-fluid{
	background: #f8f8f8;
}
main > .container-fluid > .container{
	background: white;
}
main > .container-fluid > .container >.row>.col-md-12>.thumbnail{
	padding-bottom: 10px;
}
main .thumbnail {
	border: 0;
	margin-bottom: 0;
	
	padding: 0;
}

main .text:nth-child(1){
	margin-right: 10px;
}
main .text {
	width: 110px;
	height: 40px;
	background: #2B2B2B;
	text-align: center;
	line-height: 40px;
	float: left;
	position: relative;
	top: 20px;
	color: white;
	font-size: 18px;
}

main .lazy {
	width: 100%;
	margin-bottom: 0px;
}
main h3{
	margin-bottom: 0;
	margin-top: 50px;
}
